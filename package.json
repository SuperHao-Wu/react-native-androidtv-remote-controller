{
  "name": "react-native-androidtv-remote",
  "version": "1.0.1",
  "description": "Port of AndroidTV Remote to react native",
  "main": "dist/index.js",
  "files": [
    "dist",
    "example",
    "src"
  ],
  "engines": {
    "node": ">=14.18.1"
  },
  "scripts": {
    "babel": "babel ./src/*.* --out-dir dist --delete-dir-on-start -D && babel ./src/certificate/*.* --out-dir dist/certificate -D && babel ./src/pairing/*.* --out-dir dist/pairing -D && babel ./src/remote/*.* --out-dir dist/remote -D",
    "test": "jest",
    "appium:ios": "wdio run ./appium/wdio.conf.js",
    "appium:start": "appium server --address localhost --port 4723 --relaxed-security --allow-cors",
    "appium:doctor": "appium doctor --ios"
  },
  "keywords": [
    "androidtv",
    "googlecast",
    "remote",
    "freebox",
    "freebox-player-pop",
    "freebox-mini-4k"
  ],
  "author": "@vricosti",
  "license": "MIT",
  "dependencies": {
    "node-forge": "^1.3.1",
    "protobufjs": "^7.3.0",
    "react-native-tcp-socket": "^6.3.0"
  },
  "peerDependencies": {
    "react-native": ">=0.60.0",
    "react-native-modpow": "^1.1.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.16.7",
    "@babel/core": "^7.16.7",
    "@babel/preset-env": "^7.28.0",
    "@wdio/appium-service": "^9.18.4",
    "@wdio/cli": "^9.18.4",
    "@wdio/local-runner": "^9.18.4",
    "@wdio/mocha-framework": "^9.18.0",
    "@wdio/spec-reporter": "^9.18.0",
    "appium-xcuitest-driver": "^9.9.6",
    "babel-jest": "^30.0.4",
    "babel-plugin-transform-import-meta": "^2.1.0",
    "chai": "^5.2.1",
    "core-js": "^3.44.0",
    "jest": "^30.0.4",
    "react-native-modpow": "^1.1.0",
    "selfsigned": "^3.0.0",
    "webdriverio": "^9.18.4"
  },
  "type": "commonjs",
  "homepage": "https://github.com/vricosti/react-native-androidtv-remote",
  "bugs": {
    "url": "https://github.com/vricosti/react-native-androidtv-remote/issues"
  }
}
